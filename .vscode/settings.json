{
	"latex-workshop.latex.outDir": "%DIR%/build",
	"latex-workshop.latex.autoClean.run": "onBuilt",
	"latex-workshop.view.pdf.viewer": "tab",
	"latex-workshop.latex.magic.args": [
		"-output-directory=%DIR%/build",
		"%DOC%"
	],
	"files.exclude": {
		".devcontainer": true,
		".github": true,
	},
	"editor.formatOnSave": true,
	"ltex.language": "en-US",
	"ltex.enabled": false
}
